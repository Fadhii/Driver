$n=$_GET['t1'];
$t=$n;
$s=0;
while($n>0)
{
$r=$n % 10;
$s=$s+($r*$r*$r);
$n=$n/10;
}
if($t==$s)
echo("The given number is Amstrong");
else
echo("The given number is Not Amstrong");
